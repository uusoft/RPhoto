<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>publicAlbums</short-name>
    <display-name>publicAlbums</display-name>
    <description>List all public albums
    </description>
    <tag>
        <name>publicAlbums</name>
        <tag-class>com.raysmond.album.PublicAlbumsTag</tag-class>
        <body-content>empty</body-content>
        <small-icon></small-icon>
        <large-icon></large-icon>
        <description></description>
        <attribute> 
        	<name>page</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
         <attribute> 
        	<name>pageSize</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute> 
        	<name>row</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute> 
        	<name>showPager</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute> 
        	<name>nameFilter</name>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
        <example></example>      
    </tag>
</taglib>
